<?php

namespace Sati\ControleSatiBundle\Entity;

/**
 * Boleto
 */
class Boleto
{
    /**
     * @var integer
     */
    private $bolIdboleto;

    /**
     * @var string
     */
    private $bolCodigobarra;

    /**
     * @var integer
     */
    private $fkUsuId;

    /**
     * @var \DateTime
     */
    private $bolDatavencimento;

    /**
     * @var float
     */
    private $bolValor;

    /**
     * @var integer
     */
    private $bolStatus;

    /**
     * @var \Sati\ControleSatiBundle\Entity\Eventos
     */
    private $fkEventeventos;


    /**
     * Get bolIdboleto
     *
     * @return integer
     */
    public function getBolIdboleto()
    {
        return $this->bolIdboleto;
    }

    /**
     * Set bolCodigobarra
     *
     * @param string $bolCodigobarra
     *
     * @return Boleto
     */
    public function setBolCodigobarra($bolCodigobarra)
    {
        $this->bolCodigobarra = $bolCodigobarra;

        return $this;
    }

    /**
     * Get bolCodigobarra
     *
     * @return string
     */
    public function getBolCodigobarra()
    {
        return $this->bolCodigobarra;
    }

    /**
     * Set fkUsuId
     *
     * @param integer $fkUsuId
     *
     * @return Boleto
     */
    public function setFkUsuId($fkUsuId)
    {
        $this->fkUsuId = $fkUsuId;

        return $this;
    }

    /**
     * Get fkUsuId
     *
     * @return integer
     */
    public function getFkUsuId()
    {
        return $this->fkUsuId;
    }

    /**
     * Set bolDatavencimento
     *
     * @param \DateTime $bolDatavencimento
     *
     * @return Boleto
     */
    public function setBolDatavencimento($bolDatavencimento)
    {
        $this->bolDatavencimento = $bolDatavencimento;

        return $this;
    }

    /**
     * Get bolDatavencimento
     *
     * @return \DateTime
     */
    public function getBolDatavencimento()
    {
        return $this->bolDatavencimento;
    }

    /**
     * Set bolValor
     *
     * @param float $bolValor
     *
     * @return Boleto
     */
    public function setBolValor($bolValor)
    {
        $this->bolValor = $bolValor;

        return $this;
    }

    /**
     * Get bolValor
     *
     * @return float
     */
    public function getBolValor()
    {
        return $this->bolValor;
    }

    /**
     * Set bolStatus
     *
     * @param integer $bolStatus
     *
     * @return Boleto
     */
    public function setBolStatus($bolStatus)
    {
        $this->bolStatus = $bolStatus;

        return $this;
    }

    /**
     * Get bolStatus
     *
     * @return integer
     */
    public function getBolStatus()
    {
        return $this->bolStatus;
    }

    /**
     * Set fkEventeventos
     *
     * @param \Sati\ControleSatiBundle\Entity\Eventos $fkEventeventos
     *
     * @return Boleto
     */
    public function setFkEventeventos(\Sati\ControleSatiBundle\Entity\Eventos $fkEventeventos = null)
    {
        $this->fkEventeventos = $fkEventeventos;

        return $this;
    }

    /**
     * Get fkEventeventos
     *
     * @return \Sati\ControleSatiBundle\Entity\Eventos
     */
    public function getFkEventeventos()
    {
        return $this->fkEventeventos;
    }
}
